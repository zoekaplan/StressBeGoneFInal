<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zoerebeccakaplan.stressbegone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstQuestion"
            android:label="@string/title_activity_first_question"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FeelBetter" />
        <activity android:name=".SecondQuestion" />
        <activity android:name=".FirstAnswer" />
        <activity
            android:name=".SecondAnswer"
            android:label="@string/title_activity_second_answer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ThirdQuestion"
            android:label="@string/title_activity_third_question"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ThirdAnswer" />
        <activity android:name=".FourthQuestion" />
        <activity android:name=".FourthAnswer" />
        <activity android:name=".FifthQuestion" />
        <activity android:name=".FifthAnswer" />
        <activity android:name=".SixthQuestion"></activity>
    </application>

</manifest>